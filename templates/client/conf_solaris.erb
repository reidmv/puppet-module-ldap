# Do not edit this file manually; your changes will be lost.Please use ldapclient (1M) instead.
#
NS_LDAP_FILE_VERSION= 2.0
NS_LDAP_SERVERS= <%= [@server_list].flatten.join(', ') %>
NS_LDAP_SEARCH_BASEDN= <%= @default_search_base %>
NS_LDAP_AUTH= <%= @authentication_method %>
<% if @service_search_descriptors && @service_search_descriptors != :undef -%>
<%=
  @service_search_descriptors.map do |d|
    "NS_LDAP_SERVICE_SEARCH_DESC= #{d}\n"
  end.to_s
-%>
<% end -%>
<% if @attribute_maps && @attribute_maps != :undef -%>
<%=
  @attribute_maps.map do |m|
    "NS_LDAP_ATTRIBUTEMAP= #{m}\n"
  end.to_s
-%>
<% end -%>
<% if @service_authentication_methods && @service_authentication_methods != :undef -%>
<%=
  @service_authentication_methods.map do |m|
    "NS_LDAP_SERVICE_AUTH_METHOD= #{m}\n"
  end.to_s
-%>
<% end -%>
<% if @options && @options != :undef -%>
<%= @options.map{|o| "#{o}\n" }.to_s -%>
<% end -%>
